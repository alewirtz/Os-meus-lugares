<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.osmeuslugares"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="19" />
<uses-permission android:name="android.permission.CALL_PHONE"/>
<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ale.osmeuslugares.modelo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
     
        <activity
            android:name="com.ale.osmeuslugares.modelo.AcercaDeActivity"
            android:label="@string/title_activity_acerca_de" >
        </activity>
        <activity
            android:name="com.ale.osmeuslugares.modelo.PreferenciasActivity"
            android:label="@string/title_activity_preferencias" >
        </activity>
        <activity
            android:name="com.ale.osmeuslugares.modelo.EditarLugarActivity"
            android:label="@string/title_activity_edit_lugar" >
        </activity>
        <activity
            android:name="com.ale.osmeuslugares.modelo.ListCategoriasActivity"
            android:label="@string/title_activity_list_categorias" >
        </activity>
        <activity
            android:name="com.ale.osmeuslugares.modelo.EditarCategoriaActivity"
            android:label="@string/title_activity_edit_categoria" >
        </activity>
    </application>

</manifest>
